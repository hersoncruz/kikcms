<!DOCTYPE html>
<html lang="{{ langCode }}">
<head>
    <meta charset="utf-8">
    <title>KikCMS - {% block title title %}</title>
    <link rel="shortcut icon" href="/cmsassets/favicon.ico"/>
    <link rel="apple-touch-icon" href="/cmsassets/icons/kikcms-icon-76.png">
    <link rel="apple-touch-icon" sizes="76x76" href="/cmsassets/icons/kikcms-icon-76.png">
    <link rel="apple-touch-icon" sizes="120x120" href="/cmsassets/icons/kikcms-icon-120.png">
    <link rel="apple-touch-icon" sizes="152x152" href="/cmsassets/icons/kikcms-icon-152.png">
    {{ stylesheetLink('//fonts.googleapis.com/css?family=Lato:300,400,700') }}
    {{ stylesheetLink('cmsassets/css/vendor.css') }}
    {{ stylesheetLink('cmsassets/css/cms.css?v3') }}
    {{ assets.outputCss() }}
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="robots" content="noindex">
</head>
<body class="{% block bodyClass '' %}">
<div id="overlay"></div>
{% block content %}{% endblock %}
{{ javascriptInclude('cmsassets/js/vendor.js') }}
{{ javascriptInclude('cmsassets/js/cms.js?v=10') }}
<data id="kikCmsJsSettings" value='{{ jsSettings|json_encode|raw }}'></data>
<script nonce="{{ cspNonce }}">
    {% for key in jsTranslations %}
    KikCMS.translations['{{ key }}'] = {{ tl(key)|json_encode|raw }};
    {% endfor %}
</script>
{{ assets.outputJs() }}
</body>
</html>