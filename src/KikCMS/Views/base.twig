<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>KikCMS - {% block title %}{% endblock %}</title>
    <link rel="shortcut icon" href="/cmsassets/favicon.ico" />
    <link rel="apple-touch-icon" href="/cmsassets/icons/kikcms-icon-76.png">
    <link rel="apple-touch-icon" sizes="76x76" href="/cmsassets/icons/kikcms-icon-76.png">
    <link rel="apple-touch-icon" sizes="120x120" href="/cmsassets/icons/kikcms-icon-120.png">
    <link rel="apple-touch-icon" sizes="152x152" href="/cmsassets/icons/kikcms-icon-152.png">
    {{ stylesheetLink('//fonts.googleapis.com/css?family=Lato:300,700') }}
    {{ stylesheetLink('cmsassets/css/bootstrap.min.css') }}
    {{ stylesheetLink('cmsassets/css/bootstrap-kikcms.css') }}
    {{ stylesheetLink('cmsassets/css/default.css') }}
    {{ assets.outputCss() }}
    {% block cssAssets %}{% endblock %}
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Your invoices">
    <meta name="author" content="Phalcon Team">
</head>
<body>
<div id="notFading">
    <div id="cmsLoader"><img src="/cmsassets/icons/loader.svg"><span>{{ tl('cms.loading') }}</span></div>
    <div id="ajaxDebugger" onclick="$(this).hide();"></div>
</div>
{% block content %}{% endblock %}
{{ javascriptInclude('cmsassets/js/jquery.min.js') }}
{{ javascriptInclude('cmsassets/js/bootstrap.min.js') }}
{{ javascriptInclude('cmsassets/js/utils.js') }}
<script>
    KikCMS.isDev = {{ config('application.env') == 'dev' ? 'true' : 'false' }};
    KikCMS.errorMessages = {{ tl('error')|json_encode|raw }};
</script>
<script>
    {% for key in dataTableJsTranslations %}
        KikCMS.translations['dataTable.{{ key }}'] = "{{ tl('dataTable.' ~ key) }}";
    {% endfor %}
</script>
{{ assets.outputJs() }}
{% block jsAssets %}{% endblock %}
</body>
</html>